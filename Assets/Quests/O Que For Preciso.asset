    objsToInstantiate: []
    objsToDestroy: []
    dialogoDaCondicao:
      dialogueLines:
      - '#Tyva|Iovelik#: Fique tranquilo, estou aqui para te libertar. O chefe da
        sua vila me enviou.'
      - "Curandeiro: Obrigado! Mas o chefe lhe enviou? N\xE3o pensei que ele cogitaria..."
      - "#Tyva|Iovelik#: Pedir ajuda a um mesti\xE7o? Sim eu sei. Volte para a vila,
